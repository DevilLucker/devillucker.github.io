local function onNotif(text)
    if string.find(text, "Max backpack space! Go sell!") then
        getgenv().InventoryFull = true
    end
    if string.find(text, "You cannot plant more than 800 seeds :(") then
        getgenv().PlantsFull = true
    end 
end

